<div class="form-group roll-bonuses editable-icon-input">
    <label class="header">{{label}}</label>
    <div class="form-fields">
        {{#each current}}
        <li data-index="{{@index}}" data-tooltip="{{this.name}}">
            <img src="{{this.img}}">
            <span class="icon-label">{{this.name}}</span>
        </li>
        {{/each}}
    </div>
    <a><i class="fas fa-edit"></i></a>
</div>
