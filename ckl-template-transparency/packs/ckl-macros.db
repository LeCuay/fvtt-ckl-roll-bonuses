{"name":"Disable Aura","type":"script","author":"nAIClXMrmfpoVqhM","img":"icons/svg/cancel.svg","scope":"global","command":"if (!state) {\n    const key = `${token.name}-${item.name}`;\n    if (Sequencer) {\n        Sequencer.EffectManager.endEffects({object: token, name: key });\n    }\n\n    const templateKey = `template_${key}`;\n    const currentId = await token.document.getFlag('world', templateKey)\n    if (currentId) {\n        const currentTemplate = canvas.templates.get(currentId);\n        if (currentTemplate) {\n            currentTemplate.delete();\n        }\n    }\n}","folder":null,"sort":0,"permission":{"default":0,"nAIClXMrmfpoVqhM":3},"flags":{"core":{"sourceId":"Macro.XAcCdH5nKDYNgrSU"}},"_id":"0CnfHtmYcBEVs3u5"}
